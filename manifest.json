{
  "manifest_version": 2,

  "name": "One Click Clear Cookies",
  "description": "One Click Clear Cookies",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "tabs",
   "cookies",
   "*://*/*"
   ]
}
